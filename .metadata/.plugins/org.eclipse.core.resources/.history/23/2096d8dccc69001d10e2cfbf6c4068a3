package Test;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;

import org.apache.poi.xssf.usermodel.XSSFSheet;
import org.apache.poi.xssf.usermodel.XSSFWorkbook;
public class Data {

	File src;
	FileInputStream fis;
	XSSFWorkbook xsf;
	XSSFSheet sheet;
	
  public void DataReadTest()throws Exception {
	  
	   src = new File("C:\\Users\\Domin\\eclipse-workspace\\AxaIreland\\src\\test\\java\\Test\\Data.xlsx");
	   fis= new FileInputStream(src);
	   xsf= new XSSFWorkbook(fis);
	   sheet = xsf.getSheetAt(0);
	  	 
  }
   public int SetRow(int row) {
	   
	   return row;
   }
  
  String Registration= sheet.getRow(row).getCell(1).getStringCellValue();
	 String BusinessPurpouse= sheet.getRow(row).getCell(2).getStringCellValue();
	 String TypeOfBusiness= sheet.getRow(row).getCell(3).getStringCellValue();
	 String KM= sheet.getRow(row).getCell(4).getStringCellValue();
	 String Male= sheet.getRow(row).getCell(5).getStringCellValue();
	 String Name= sheet.getRow(row).getCell(6).getStringCellValue();
	 String Surname= sheet.getRow(row).getCell(7).getStringCellValue();
	 String Day= sheet.getRow(row).getCell(8).getStringCellValue();
	 String Month= sheet.getRow(row).getCell(9).getStringCellValue();
	 String email= sheet.getRow(row).getCell(10).getStringCellValue();
	 String Phone= sheet.getRow(row).getCell(11).getStringCellValue();
	 String Employment= sheet.getRow(row).getCell(12).getStringCellValue();
	 String Occupation= sheet.getRow(row).getCell(13).getStringCellValue();
	 String Adress= sheet.getRow(row).getCell(14).getStringCellValue();
	 String TypeofHouseHold= sheet.getRow(row).getCell(15).getStringCellValue();
	 String Licence= sheet.getRow(row).getCell(16).getStringCellValue();
	 String EssentialDriving= sheet.getRow(row).getCell(17).getStringCellValue();
